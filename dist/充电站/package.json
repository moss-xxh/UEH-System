{
  "name": "charging-station-saas",
  "version": "0.1.0",
  "private": true,
  "description": "Multi-tenant SaaS platform for electric vehicle charging network operators",
  "engines": {
    "node": ">=20.11.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "test": "turbo test",
    "clean": "turbo clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo build && changeset publish"
  },
  "devDependencies": {
    "turbo": "^1.13.0",
    "@changesets/cli": "^2.26.0",
    "eslint": "^8.57.0",
    "prettier": "^3.0.0",
    "typescript": "^5.4.0"
  },
  "author": "Charging Station SaaS Team",
  "license": "UNLICENSED"
}